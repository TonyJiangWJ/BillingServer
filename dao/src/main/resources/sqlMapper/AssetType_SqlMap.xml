<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="${target.daoPackage}.mapper.AssetTypeMapper">
    <sql id="all">
        id id,
        type_code typeCode,
        parent_code parentCode,
        type_desc typeDesc,
        create_time createTime,
        modify_time modifyTime,
    </sql>
    <insert id="insert" useGeneratedKeys="true" keyProperty="id" parameterType="com.tony.billing.entity.Asset">
        INSERT INTO t_asset_type (
        <trim suffixOverrides=",">
            <if test=" id!=null">
                id,
            </if>
            <if test=" typeCode!=null and typeCode!=''">
                type_code,
            </if>
            <if test=" parentCode!=null and parentCode!=''">
                parent_code,
            </if>
            <if test=" typeDesc!=null and typeDesc!=''">
                type_desc,
            </if>
            <if test=" createTime!=null">
                create_time,
            </if>
            <if test=" modifyTime!=null">
                modify_time,
            </if>
        </trim>
        ) VALUES (
        <trim suffixOverrides=",">
            <if test=" id!=null">
                #{id,jdbcType=${filed.jdbcType},
            </if>
            <if test=" typeCode!=null and typeCode!=''">
                #{typeCode,jdbcType=${filed.jdbcType},
            </if>
            <if test=" parentCode!=null and parentCode!=''">
                #{parentCode,jdbcType=${filed.jdbcType},
            </if>
            <if test=" typeDesc!=null and typeDesc!=''">
                #{typeDesc,jdbcType=${filed.jdbcType},
            </if>
            <if test=" createTime!=null">
                #{createTime,jdbcType=${filed.jdbcType},
            </if>
            <if test=" modifyTime!=null">
                #{modifyTime,jdbcType=${filed.jdbcType},
            </if>
        </trim>
        )
    </insert>
    <update id="update" parameterType="com.tony.billing.entity.Asset">
        UPDATE t_asset_type
        <trim prefix="SET" suffixOverrides=",">
            <if test=" id!=null">
                id=#{id,jdbcType=${filed.jdbcType},
            </if>
            <if test=" typeCode!=null and typeCode!=''">
                type_code=#{typeCode,jdbcType=${filed.jdbcType},
            </if>
            <if test=" parentCode!=null and parentCode!=''">
                parent_code=#{parentCode,jdbcType=${filed.jdbcType},
            </if>
            <if test=" typeDesc!=null and typeDesc!=''">
                type_desc=#{typeDesc,jdbcType=${filed.jdbcType},
            </if>
            <if test=" createTime!=null">
                create_time=#{createTime,jdbcType=${filed.jdbcType},
            </if>
            <if test=" modifyTime!=null">
                modify_time=#{modifyTime,jdbcType=${filed.jdbcType},
            </if>
        </trim>
        WHERE id=#{id, jdbcType=BIGINT}
    </update>
    <select id="page" parameterType="java.util.Map" resultType="com.tony.billing.entity.Asset">
        SELECT
        <include refid="all"/>
        FROM t_asset_type
        <trim prefix="WHERE" prefixOverrides="and">
            <if test=" id!=null">
                and id=#{id,jdbcType=${filed.jdbcType},
            </if>
            <if test=" typeCode!=null and typeCode!=''">
                and type_code=#{typeCode,jdbcType=${filed.jdbcType},
            </if>
            <if test=" parentCode!=null and parentCode!=''">
                and parent_code=#{parentCode,jdbcType=${filed.jdbcType},
            </if>
            <if test=" typeDesc!=null and typeDesc!=''">
                and type_desc=#{typeDesc,jdbcType=${filed.jdbcType},
            </if>
            <if test=" createTime!=null">
                and create_time=#{createTime,jdbcType=${filed.jdbcType},
            </if>
            <if test=" modifyTime!=null">
                and modify_time=#{modifyTime,jdbcType=${filed.jdbcType},
            </if>
        </trim>
        ORDER BY
        <if test="orderBy != null">
            \${orderBy} \${sort}
        </if>
        <if test="orderBy == null or orderBy == '' ">
            id \${sort}
        </if>
        LIMIT #{index} , #{offset}
    </select>

    <select id="list" parameterType="com.tony.billing.entity.Asset" resultType="com.tony.billing.entity.Asset">
        SELECT
        <include refid="all"/>
        FROM t_asset_type
        <trim prefix="WHERE" prefixOverrides="and">
            <if test=" id!=null">
                and id=#{id,jdbcType=${filed.jdbcType},
            </if>
            <if test=" typeCode!=null and typeCode!=''">
                and type_code=#{typeCode,jdbcType=${filed.jdbcType},
            </if>
            <if test=" parentCode!=null and parentCode!=''">
                and parent_code=#{parentCode,jdbcType=${filed.jdbcType},
            </if>
            <if test=" typeDesc!=null and typeDesc!=''">
                and type_desc=#{typeDesc,jdbcType=${filed.jdbcType},
            </if>
            <if test=" createTime!=null">
                and create_time=#{createTime,jdbcType=${filed.jdbcType},
            </if>
            <if test=" modifyTime!=null">
                and modify_time=#{modifyTime,jdbcType=${filed.jdbcType},
            </if>
        </trim>
    </select>

    <select id="getById" parameterType="Long" resultType="com.tony.billing.entity.Asset">
        SELECT
        <include refid="all"/>
        FROM t_asset_type WHERE id=#{id,jdbcType=BIGINT}
    </select>
</mapper>